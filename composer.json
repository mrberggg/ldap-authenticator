{
    "name": "berg/ldap-authenticator",
    "description": "A package for quickly setting up LDAP authentication",
    "keywords": [ "LDAP", "PHP", "laravel" ],
    "homepage": "https://github.com/mrberggg/ldap-authenticator",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Matt Berg",
            "email": "mrberggg@gmail.com",
            "homepage": "https://github.com/mrberggg"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-ldap": "^2.5",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-session": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "mockery/mockery": "0.9.*",
        "illuminate/support": "5.*",
        "illuminate/auth": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Berg\\LdapAuthenticator\\": "src"
        }
    }
}
